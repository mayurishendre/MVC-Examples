<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My Portfolio </title> @*...change 1*@
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Heading", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse" pb="10">
                <ul class="nav navbar-nav">
                    @*<li>@Html.ActionLink("Home", "Index", "Home")</li>
                    <li>@Html.ActionLink("About", "About", "Home")</li>
                    <li>@Html.ActionLink("Contact", "Contact", "Home")</li>*@
                    <li>@Html.ActionLink("CareerObjectives", "CareerObjectives", "CareerObjectives")</li>

                    <li>@Html.ActionLink("TechnicalSkills", "TechnicalSkills", "TechnicalSkills")</li>
                    <li>@Html.ActionLink("Education", "Education", "Education")</li>
                    <li>@Html.ActionLink("ProfessionalSummary", "ProfessionalSummary", "ProfessionalSummary")</li>
                    <li>@Html.ActionLink("ProfessionalExperience", "ProfessionalExperience", "ProfessionalExperience")</li>
                    @*<li>@Html.ActionLink("ProjectDetails", "ProjectDetails", "ProjectDetails")</li>
                    <li>@Html.ActionLink("RolesAndResponsibility", "RolesAndResponsibility", "RolesAndResponsibility")</li>
                    <li>@Html.ActionLink("PersonalDetails", "PersonalDetails", "PersonalDetails")</li>
                    <li>@Html.ActionLink("Dclaration", "Dclaration", "Declaration")</li>*@
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content" pt="5">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
